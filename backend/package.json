{
  "name": "ai-agent-backend",
  "version": "1.0.0",
  "description": "Backend for AI Knowledge Heirloom - A gift from senior developer to the next generation",
  "main": "dist/app.js",
  "scripts": {
    "start": "node dist/app.js",
    "start:prod": "node dist/app.js",
    "dev": "nodemon src/app.ts",
    "build": "tsc",
    "migrate": "npx prisma migrate dev",
    "generate": "npx prisma generate",
    "studio": "npx prisma studio",
    "seed": "npx tsx prisma/seed/data.ts",
    "db:reset": "npx prisma migrate reset --force && npm run seed",
    "setup": "npx tsx scripts/setup-database.ts",
    "db:init": "npm run migrate && npm run generate && npm run setup"
  },
  "keywords": [
    "ai",
    "assistant",
    "knowledge",
    "heirloom",
    "hackathon"
  ],
  "author": "Team Knowledge Heirloom",
  "license": "MIT",
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@huggingface/hub": "^2.4.1",
    "@prisma/client": "^5.6.0",
    "@types/bcrypt": "^6.0.0",
    "@types/multer": "^2.0.0",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "cheerio": "^1.1.2",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "form-data": "^4.0.4",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsdom": "^26.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "openai": "^4.20.1",
    "pdf-parse": "^1.1.1",
    "python-shell": "^5.0.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.9.0",
    "@types/pdf-parse": "^1.1.5",
    "nodemon": "^3.0.1",
    "prisma": "^5.6.0",
    "ts-node": "^10.9.1",
    "tsx": "^4.20.4",
    "typescript": "^5.2.2"
  }
}
